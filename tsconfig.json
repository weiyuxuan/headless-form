{
  "compilerOptions": {
    "esModuleInterop": true,
    "declaration": true,
    "declarationDir": "./libs",
    "noEmitOnError": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "target": "ES5",
    "jsx": "react",
    "lib": ["ESNext", "DOM"],
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
  ]
}
